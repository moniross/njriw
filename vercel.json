{
    "version": 2,
    "builds": [
      {
        "src": "public/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "api/send-to-telegram.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  